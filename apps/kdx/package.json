    "@tanstack/react-table": "^8.16.0",
    "@trpc/client": "catalog:",
    "@trpc/react-query": "catalog:",
    "@trpc/server": "catalog:",
    "next": "catalog:",
    "next-safe-action": "7.1.4-experimental.1",
    "next-themes": "^0.3.0",
    "react": "catalog:react18",
    "react-day-picker": "^8.10.0",
    "react-dom": "catalog:react18",
    "react-hook-form": "^7.52.1",
    "superjson": "2.2.1",
